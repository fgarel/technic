class sommet():
    """
    sommet
    """
    def __init__(self, x=0, y=0):
        self.x = x
        self.y = y

    def __str__():
        print(""%(x))
class Point:
    """ Create a new Point, at coordinates x, y """

        def __init__(self, x=0, y=0):
            """ Create a new point at x, y """
            self.x = x
            self.y = y

                            def distance_from_origin(self):
                                                                """ Compute my distance from the origin """
                                                                        return ((self.x ** 2) + (self.y ** 2)) ** 0.5

                                                                        def __str__(self):
                                                                                    return "({0}, {1})".format(self.x, self.y)

                                                                                    def halfway(self, target):
                                                                                                """ Return the halfway point between myself and the target """
                                                                                                        mx = (self.x + target.x)/2
                                                                                                                my = (self.y + target.y)/2
                                                                                                                        return Point(mx, my)


                                                                                                                    class Triangle:
                                                                                                                            """ Create a new Triangle """

                                                                                                                                def __init__(self, Sommet1, Sommet2, Sommet3):
                                                                                                                                            """ Create a new Triangle """
                                                                                                                                                    self.Sommet1 = Sommet1
                                                                                                                                                            self.Sommet2 = Sommet2
                                                                                                                                                                    self.Sommet3 = Sommet3

                                                                                                                                                                        def __str__(self):
                                                                                                                         return "({0}, {1}), ({2}, {3}), ({4}, {5})".format(self.Sommet1.x, self.Sommet1.y, self.Sommet2.x, self.Sommet2.y, self.Sommet3.x, self.Sommet3.y)

                                                                                                                                                                                    Points = []
                                                                                                                                                      Points.append(Point(3, 4))
                                                                                                                                                                                    Points.append(Point(5, 12))
                                                                                                                                                                                    #Points[2] = p.halfway(q)
                                                                                                                                                                                    Points.append(Point(12, 3))

                                                                                                                                                                                    Triangles = []
                                                                                                                                                                                    Triangles.append(Triangle(Points[0], Points[1], Points[2]))
                                                                                                                                                                                    print Triangles[0]

