	<xs:complexType name="informationsDT">
		<xs:sequence>
			<xs:choice>
				<xs:element name="noConsultationDuTeleserviceSeize" type="t:numDossierSeize">
					<xs:annotation>
						<xs:documentation>Numero de consultation du dossier sur 16
