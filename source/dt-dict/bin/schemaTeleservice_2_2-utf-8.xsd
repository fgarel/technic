		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element name="DT" type="t:informationsDT" />
					<xs:element name="DICT" type="t:informationsDICT" />
					<xs:element name="dtDictConjointes" type="t:informationsDtDictConjointes" />
